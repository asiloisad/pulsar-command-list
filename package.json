{
  "name": "command-list",
  "description": "Find and run available commands",
  "repository": "https://github.com/asiloisad/pulsar-command-list",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "activationCommands": {
    "atom-workspace": [
      "command-list:toggle",
      "command-list:show-hidden-commands"
    ]
  },
  "configSchema": {
    "recentCount": {
      "title": "Number of Recent Items",
      "description": "How many recently used commands to keep at the top of the list.",
      "type": "integer",
      "default": 5,
      "minimum": 0
    }
  },
  "dependencies": {
    "pulsar-select-list": "^1.0.0"
  }
}
